<?xml version="1.0" encoding="utf-8"?>
<odoo>
   
   <record id="flour_extraction_view" model="ir.ui.view">
      <field name="name">flour.extraction.lista.view</field>
      <field name="model">mrp.bom</field>
      <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
      <field name="arch" type="xml">
         <xpath expr="//notebook/page/field[@name='bom_line_ids']/tree" position="inside">
            <field name="product_principal_id"/>
            <field name="emp_primary"/>
            <field name="emp_secondary"/>
            <field name="cinta_codificadora"/>
            <field name="teflon"/>
         </xpath>
      </field>
   </record>
</odoo>


