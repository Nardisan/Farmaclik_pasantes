#. module: l10n_ve_retencion_iva
#: model:mail.template,body_html:l10n_ve_retencion_iva.email_template_wh_iva
msgid ""
"\n"
"            <p>Estimados ${object.partner_id.name}\n"
"            % set access_action = object.with_context(force_website=True)."
"get_access_action()\n"
"            % set doc_name = 'Comprobante de Retención IVA'\n"
"\n"
"            % if object.partner_id.parent_id:\n"
"                (<i>${object.partner_id.parent_id.name}</i>)\n"
"            % endif\n"
"            ,</p>\n"
"            <p>\n"
"            Adjuntamos su ${doc_name} Número <strong>${object.number}</"
"strong>\n"
"            % if object.origin:\n"
"            (with reference: ${object.number} )\n"
"            % endif\n"
"            por un total <strong>${\"%.2f\" % object.total_tax_ret} "
"${object.company_id.currency_id.symbol}</strong>\n"
"            de ${object.company_id.name}\n"
"            </p>\n"
"\n"
"                <br/><br/>\n"
"            % if is_online:\n"
"                <center>\n"
"                    <a href=\"${access_url}\" style=\"background-color: "
"#1abc9c; padding: 20px; text-decoration: none; color: #fff; border-radius: "
"5px; font-size: 16px;\" class=\"o_default_snippet_text\">${access_name}</"
"a>\n"
"                    <br/><br/>\n"
"                    <span style=\"color:#888888\">(or view attached PDF)</"
"span>\n"
"                </center>\n"
"                <br/>\n"
"            % endif\n"
"\n"
"            <p>Si tiene alguna pregunta, puede ponerse en contacto con "
"nosotros.</p>\n"
"            <p>Muchas Gracias,</p>\n"
"\n"
"            <p style=\"color:#eeeeee;\">\n"
"            </p>\n"
"            \n"
"          "
